{
  "name": "disco",
  "version": "0.0.0",
  "description": "deploy and manage your web projects",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/letsdiscodev/cli",
  "bugs": "https://github.com/letsdiscodev/cli/issues",
  "repository": "letsdiscodev/cli",
  "license": "MIT",
  "author": "The Disco Team",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "disco": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "oclif readme && git add README.md",
    "pack": "oclif pack tarballs"
  },
  "oclif": {
    "bin": "disco",
    "commands": "./dist/commands",
    "dirname": "disco",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "@types/ssh2-streams": "^0.1.12",
    "chalk": "^4",
    "node-ssh": "^13.1.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8.57.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
